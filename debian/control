on-avatar, geary, plymouth-theme-blankon, libgdm1, grilo-plugins-0.3, corebird, locales-all, firefox, california, bromo-theme, lsb-base, tebu-flat-icon-theme, dbus-user-session, watchdog, gnome-software, appstream-data, scim-gtk-immodule, fontconfig-infinality
